{
  "Database": {
    "Server": "localhost",
    "Port": "3306",
    "Schema": "queue",
    "User": "root",
    "Password": "root"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "outputTemplate": "{NewLine}[{Timestamp:HH:mm:ss.fff} {Level:u3}]{Scope} {Message}{NewLine}{Exception}" }
      }
    ]
  },
  "Messaging": {
    "Host": "localhost",
    "VirtualHost": "app",
    "Port": 5672,
    "User": "admin",
    "Password": "admin",
    "Durable": true,
    "TTL": 1200000,
    "Retries": 3,
    "Consuming": {
      "Queue": "app.status1",
      "BindingKey": "processing",
      "Exchange": "process",
      "Deadletter": {
        "Queue": "app.status1.deadletter",
        "RoutingKey": "deadletter",
        "Exchange": "process"
      }
    },
    "Publishing": {
      "Queue": "app.status2",
      "RoutingKey": "processing",
      "Exchange": "process"
    },
    "Error": {
      "Queue": "app.errors",
      "Exchange": "errors",
      "RoutingKey": "errors"
    }
  }
}
